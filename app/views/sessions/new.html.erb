<%= form_for :session, url: session_path do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit t('sessions.sign_in') %>
  </div>
<% end %>
<%= link_to t('sessions.reset_password'), new_password_reset_path %>
